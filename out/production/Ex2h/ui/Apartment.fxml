<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="270.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ApartmentController">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="90.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="160.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Apartment Id:" />
            <Label text="Apartment#:" GridPane.rowIndex="1" />
            <Label text="Square Feet:" GridPane.rowIndex="2" />
            <Label text="Bathrooms:" GridPane.rowIndex="3" />
            <Label text="Price:" GridPane.rowIndex="4" />
            <Label text="Updated:" GridPane.rowIndex="5" />
            <TextField fx:id="apartmentIdTextField" GridPane.columnIndex="1" />
            <TextField fx:id="apartmentNumTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="squareFeetTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="bathroomsTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="priceLabel" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label fx:id="updatedLabel" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="6">
               <children>
                  <Label fx:id="errMsgLabel" textFill="RED" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="7">
               <children>
                  <Button fx:id="saveApartment1Button" mnemonicParsing="false" onAction="#saveApartment1ButtonClicked" prefWidth="75.0" text="Save 1" />
                  <Button fx:id="saveApartment2Button" mnemonicParsing="false" onAction="#saveApartment2ButtonClicked" prefWidth="75.0" text="Save 2" />
                  <Button fx:id="saveApartment3Button" mnemonicParsing="false" onAction="#saveApartment3ButtonClicked" prefWidth="75.0" text="Save 3" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="8">
               <children>
                  <Button fx:id="displayApartment1Button" disable="true" mnemonicParsing="false" onAction="#displayApartment1ButtonClicked" prefWidth="75.0" text="Display 1" />
                  <Button fx:id="displayApartment2Button" disable="true" mnemonicParsing="false" onAction="#displayApartment2ButtonClicked" prefWidth="75.0" text="Display 2" />
                  <Button fx:id="displayApartment3Button" disable="true" mnemonicParsing="false" onAction="#displayApartment3ButtonClicked" prefWidth="75.0" text="Display 3" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
